{
  "root": true,
  "extends": ["next", "next/core-web-vitals", "prettier"],
  "rules": {
    "import/prefer-default-export": "off",
    "newline-before-return": "error",
    "no-console": "warn",
    "no-var": "error",
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ]
  }
}
